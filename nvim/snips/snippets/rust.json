{
  "errorkind": {
    "prefix": [
      "thiserror"
    ],
    "body": [
      "use std::io::Error as StdIOError;",
      "",
      "use thiserror::Error;",
      "",
      "pub type Result<T> = std::result::Result<T, ErrorKind>;",
      "",
      "#[derive(Error, Debug)]",
      "pub enum ErrorKind {",
      "    #[error(\"io error: {0}.\")]",
      "    IOError(#[from] StdIOError),",
      "}"
    ]
  }
}
