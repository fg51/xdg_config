{
  "pyright": {
    "prefix": "pyright",
    "body": [
      "[tool.pyright]",
      "include = [\"src\"]",
      "exclude = [\"**/node_modules\", \"**/__pycache__\", \"src/typestubs\" ]",
      "executionEnvironments = [ {root = \"src\"} ]",
      "#stubPath = \"src/stubs\"",
      "venvPath = \".\"",
      "venv = \".venv\""

    ]

  }
}
